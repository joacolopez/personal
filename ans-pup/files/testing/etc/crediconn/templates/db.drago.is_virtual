PATH: /etc/bind/db.drago
;Generado desde el template /etc/crediconn/templates/db.drago
;Modificado 2020-07 para plataforma sobre ProxMox de filiales.

$TTL    5m
@       IN      SOA     nodovpn%(zfilial)s.filial%(zfilial)s.bancocredicoop.coop dns.filial%(zfilial)s.bancocredicoop.coop (
                 2011122801         ; Serial
                             1h         ; Refresh
                             5m         ; Retry
                             1w         ; Expire
                             1h )       ; Negative Cache TTL
;
@                       IN      NS      nodovpn%(zfilial)s-drago
@                       IN      NS      nodovpn%(zfilial)s-vera


;----NodoVPN -------------------------------
nodovpn%(zfilial)s-drago IN A %(lan_ip_drago1)s
nodovpn%(zfilial)s-vera IN A %(lan_ip_vera1)s
nodovpn%(zfilial)s IN A %(net_prefix_drago)s.254

;----Servidores ----------------------------
spve		IN A %(net_prefix_drago)s.1 
p100%(zfilial)s	IN A %(net_prefix_drago)s.2
iDrac1		IN A %(net_prefix_drago)s.3 
iDrac2		IN A %(net_prefix_drago)s.4 
sans		IN A %(net_prefix_drago)s.5 
sapt		IN A %(net_prefix_drago)s.6 
scdn		IN A %(net_prefix_drago)s.7 
sftp		IN A %(net_prefix_drago)s.8 
