PATH: /etc/ipsec.d/%(section)s-vera-fase2.conf

conn %(section)s-vera-fase2
	right=%(concentrador_vera)s
	rightid=@concentrador-vera.bancocredicoop.coop
        rightsubnet=10.0.0.0/8
	left=%%defaultroute
	leftallowany=yes
	leftsubnet=%(net_prefix_vera)s.0/24

conn %(section)s-vera-fase2-voip
	right=%(concentrador_vera)s
	rightid=@concentrador-vera.bancocredicoop.coop
        rightsubnet=10.7.0.0/16
	rightprotoport=udp
	left=%%defaultroute
	leftallowany=yes
	leftsubnet=%(net_prefix_vera)s.168/29
	compress=no
	esp=null-sha256-modp2048!

