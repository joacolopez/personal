PATH: /etc/ipsec.d/%(section)s-drago-fase2.conf

conn %(section)s-drago-fase2
	right=%(concentrador_drago)s
	rightid=@concentrador-drago.bancocredicoop.coop
        rightsubnet=10.0.0.0/8
	left=%(ip)s
	leftsubnet=%(net_prefix_drago)s.0/24

conn %(section)s-drago-fase2-voip
	right=%(concentrador_drago)s
	rightid=@concentrador-drago.bancocredicoop.coop
        rightsubnet=10.7.0.0/16
	rightprotoport=udp
	left=%(ip)s
	leftsubnet=%(net_prefix_drago)s.168/29
	compress=no
	esp=null-sha256-modp2048!

